{
  "source": {
    "include": ["../server", "../client"],
    "includePattern": ".+\\.js(doc)?$",
    "excludePattern": "(^|\\/|\\\\)_|server[\\\\|//]node_modules|js[\\\\|//]lib|css[\\\\|//]lib"
  },
  "opts": {
    "template": "./docstrap/template",
    "destination": "../docs",
    "recurse": true
  },
  "templates": {
    "default": {
      "outputSourceFiles": true
    },
    "systemName": "og docs",
    "navType": "vertical",
    "theme": "cerulean",
    "linenums": false,
    "collapseSymbols": false,
    "inverseNav": true
  },
  "tags": {
    "allowUnknownTags": "true"
  },
  "plugins": [ "plugins/markdown" ],
  "markdown": {
    "parser": "gfm",
    "githubRepoName": "opengash",
    "githubRepoOwner": "msafi"
  }
}